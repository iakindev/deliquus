{
  "name": "deliquus",
  "version": "0.3.0",
  "description": "A coverage tool that help you find missing tests, stories etc.",
  "main": "deliquus.js",
  "scripts": {
    "start": "node dist/deliquus.js",
    "dev": "nodemon --exec babel-node --extensions '.js,.ts' src/deliquus.ts",
    "dev:debug": "DEBUG=1 yarn dev",
    "build": "yarn run babel --extensions '.js,.ts' --ignore '**/__tests__' src -d dist --copy-files",
    "test": "DEBUG=1 jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iakindev/deliquus.git"
  },
  "keywords": [
    "cli",
    "test",
    "story",
    "husky",
    "missing tests",
    "missing stories",
    "missing files"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iakindev/deliquus/issues"
  },
  "homepage": "https://github.com/iakindev/deliquus#readme",
  "devDependencies": {
    "@atolye15/eslint-config": "^1.2.0",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/babel__core": "^7.1.12",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "cosmiconfig": "^7.0.0",
    "glob": "^7.1.6",
    "ts-node": "^9.1.1"
  }
}
